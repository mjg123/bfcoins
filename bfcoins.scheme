;; sudo apt-get install guile-1.8

(define coins (list 1 2 5 10 20 50 100 200))

(define (max-coin coins target)
  (apply max
    (filter (lambda (x) (>= target x)) coins)))

(define (calculate coins target)
  (define coin (max-coin coins target))
  (cond
    ((= coin target) (list coin))
    ((< coin target) (cons coin (calculate coins (- target coin))))))



;; then we can do like:

(calculate coins 123)
